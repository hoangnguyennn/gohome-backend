{
  "name": "gohome-backend",
  "version": "1.0.0",
  "author": "hoangnguyen <nvanhoang188@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register src/server.ts",
    "build": "rm -rf dist && tsc && tsc-alias",
    "start": "pm2 start dist/server.js -i max",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "restart": "pnpm stop && pnpm delete && pnpm start",
    "pm2": "pm2",
    "seed": "tsnd -r tsconfig-paths/register src/data/index.ts",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier --write",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn prettier:fix && yarn lint --fix"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "celebrate": "15.0.1",
    "cloudinary": "1.33.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "mongodb": "4.13.0",
    "mongoose": "6.8.2",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "pm2": "5.2.2",
    "slug": "8.2.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/jsonwebtoken": "9.0.0",
    "@types/morgan": "1.9.3",
    "@types/multer": "1.4.7",
    "@types/slug": "5.0.3",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.8.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.2",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4"
  }
}
